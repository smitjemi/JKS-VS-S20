'Math Example


Module Module1

    Sub Main()

        'Addition Operator
        Console.WriteLine("2 + 2 = ")
        Console.WriteLine(2 + 2)
        Console.ReadLine()

        'Subtraction Operator
        Console.WriteLine("4 - 2 = ")
        Console.WriteLine(4 - 2)
        Console.ReadLine()

        'Multiply Operator
        Console.WriteLine("2 * 2 = ")
        Console.WriteLine(2 * 2)
        Console.ReadLine()

        'Division Operator. Decimal 
        Console.WriteLine("25 / 3 = ")
        Console.WriteLine(25 / 3)
        Console.ReadLine()

        'Integer Division Operator. Whole Number
        Console.WriteLine("25 \ 3 = ")
        Console.WriteLine(25 \ 3)
        Console.ReadLine()

        'Modulus Operator. Remainder
        Console.WriteLine("25 Mod 3 = ")
        Console.WriteLine(25 Mod 3)
        Console.ReadLine()

        'Exponant
        Console.WriteLine("2^8 = ")
        Console.WriteLine(2 ^ 8)
        Console.ReadLine()
    End Sub

End Module
